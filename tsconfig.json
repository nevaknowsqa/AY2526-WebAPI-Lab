{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./",
    
    "jsx": "react-jsx",
    
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
        
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "isolatedModules": true
  }
}